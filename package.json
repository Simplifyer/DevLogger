{
  "name": "SimplifyerDevLogger",
  "version": "0.0.1",
  "description": "Development console log",
  "scripts": {
    "build": "npm run clear && webpack --mode=production",
    "dev": "npm run clear && webpack --mode=development",
    "start": "npm run clear && webpack-dev-server --mode=development --open",
    "clear": "del-cli dist",
    "fix": "./node_modules/.bin/eslint --fix src/**/*.{js,ts,tsx}",
    "lint": "./node_modules/.bin/eslint src/**/*.{js,ts,tsx}"
  },
  "author": "Andrey Suha",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "del-cli": "^3.0.0",
    "eslint": "^7.2.0",
    "eslint-loader": "^4.0.2",
    "html-webpack-plugin": "^3.2.0",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.5",
    "webpack": "^4.41.4",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  }
}
